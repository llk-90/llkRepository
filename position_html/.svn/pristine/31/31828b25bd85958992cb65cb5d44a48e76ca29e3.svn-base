.header{
    width:100%;
    height:75px;
    border: 0px solid #e0e4ef;
    box-shadow:-2px 2px 11px #e0e4ef;
    /*margin:8px auto;*/
    background-color: #FFFFFF;
}
#headPortrait{
}
#headPortrait img{
    border: 0px solid red;
    margin-top: 3%;
    width: 50px;
    height: 50px;
    border-radius:50%;
    position: absolute;
    left:5%;
}
