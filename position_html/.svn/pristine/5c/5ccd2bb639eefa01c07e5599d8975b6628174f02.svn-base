body{
    font-family:Helvetica,STHeiti STXihei, Microsoft JhengHei, Microsoft YaHei, Arial;
}
#child-img{
    width: 45px;
    height: 45px;
    border-radius:50%;
    float: left;
    margin-top:8px;
    margin-left:10px;
}
#child-name{
    display: block;
    width:120px;
    height: 64px;
    line-height: 64px;
    font-size: 16px;
    border:0px solid red;
    float: left;
    margin-left:18px;
}
#child-span-tatus1{
    display: block;
    height: 64px;
    line-height: 64px;
    font-size: 14px;
    border:0px solid red;
    float: right;
    margin-right:9px;
    /*color:#41c668;*/
    /*çº¢*/
    color:#ef6456;
}
#child-img-status1{
    width: 15px;
    height: 15px;
    float: right;
    border:0px solid red;
    margin-top:24px;
    margin-right:10px;
}
.jgg{
    width:30%;
    height: 29%;
    border:0px solid red;
    float:left;
    text-align:center;
}
.jgg-img{
    width:31px;
    height:31px;
    border: 0px solid blue;
    margin:15% auto;
    margin-bottom:0;
}
.jgg-span{
    display:block;
    height:17px;
    line-height:17px;
    font-size:13px;
    border:0px solid red;
    width:100%;
    margin-top:10%;
    color: #73767f;
}
#two{
    border:1px solid #e1e1e0;
    box-shadow: 0px -1px 3px #e1e1e0;
    -webkit-border-radius: 2px;
    -moz-border-radius: 2px;
    border-radius: 2px;
    width:94%;
    position: absolute;
    top:70%;
    left:3%;
    z-index: 999;
    background-color:white;
}
#three{
    border:1px solid #e1e1e0;
    box-shadow: 0px -1px 1px #e1e1e0;
    border-radius: 2px;
    text-align:center;
    border-radius: 2px;
    width:94%;
    position: absolute;
    top:93%;
    left:3%;
    z-index: 998;
    background-color:white;
}
#four{
    width:94%;
    height:4%;
    box-shadow: 0px -2px 1px #e1e1e0;
    background-color: white;
    position: absolute;
    bottom:0%;
    left:3%;
    z-index: 999;
    display: none;
}
#jiugong{
    height:270px;
}