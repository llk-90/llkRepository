.top{
width: 90%;
	height: 3rem;
	background-color: white;
	margin-left: 5%;
	margin-top: 2%;
	padding-left: 5%;
	color: #999999;
	display: flex;
	border-radius: 5px;
	flex-direction: column;
	justify-content: space-around;

}

.m-box{
	width: 90%;
	height: 15rem;
	border-radius: 5px;
	background-color: white;
	margin-left: 5%;
	margin-top: 1%;

}
.m-box-child{
	width: 90%;
	height: 25%;
	margin-left: 5%;
	padding-top: 3%;
	border-radius: 5px;
}
.m-box-child-child{
	width: 25%;
	height: 100%;
	color: #999999;
	display: flex;
	justify-content: space-around;
	border-radius: 5px;

}