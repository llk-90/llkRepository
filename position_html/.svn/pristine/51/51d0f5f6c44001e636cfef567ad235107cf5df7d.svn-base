.my_menu {
    width: 100%;
    background: #efefef;
    box-sizing: border-box;
}
.my_menu ul {
    margin-top: 0px;
    padding: 5px 0;
    overflow: hidden;
    margin-bottom: 0px;
    background: #fff;
    background-image: url("data:image/svg+xml;utf8,<svg xmlns='http://www.w3.org/2000/svg' width='100%' height='1'><rect fill='#c8c7cc' x='0' y='0.5' width='100%' height='0.5'/></svg>");
    background-repeat: no-repeat;
    background-position: 0 100%, 0 0;
    font-size: 12px;
    width: 100%;
}
.my_menu li{
    display: block;
    -webkit-flex: 1;
    -webkit-box-flex: 1;
    margin: 0;
    padding: 0;
    vertical-align: baseline;
    width:25%;
    float: left;
}
.my_menu a {
    position: relative;
    display: block;
    text-align: center;
    color: #666;
}
.my_menu a:before {
    width: 25px;
    height: 25px;
    margin: 0 auto;
    background: url(../images/iconv3.png) no-repeat;
    -webkit-background-size: 25px 125px;
    background-size: 25px 125px;
    content: '\20';
    display: block;
}
.my_menu a:after {
    display: block;
    content: '\20';
    width: 1px;
    height: 100%;
    position: absolute;
    left: -1px;
    top: 0;
    background: #efefef;
}
.my_menu .menu_2:before {
    background-position: 0 -25px;
}
.my_menu .menu_3:before {
    background-position: 0 -50px;
}
.my_menu .menu_4:before {
    background-position: 0 -75px;
}
.my_menu a span {
    position: absolute;
    left: 55%;
    top: 0;
    background: #e4393c;
    color: #fff;
    z-index: 1;
    font-size: 8px;
    min-width: 12px;
    padding: 0 2px;
    line-height: 1.5em;
    border-radius: 10px;
    -webkit-border-radius: 10px;
}
