.student{
    border:0px solid red;
    background-color:white;
    border-radius:5px;
    width:94%;
    height:330px;
    margin-left:3%;
    margin-top:-30px;
    position: relative;
}
.boem{
    width:100%;
    height:70px;
}
.perlogo{
    border:0px solid red;
    height:5%;
    width: 4%;
    position: absolute;
    margin-left:4%;
    margin-top:3%;
}
.sanjiao{
    width:0;
    height:0;
    border-top:19px solid #ffba41;
    border-right:19px solid #ffba41;
    border-bottom: 19px solid white;
    border-left:19px solid white;
    border-radius:2px;
    float:right;
}
.student span{
    font-size:4px;
    color:white;
    position: absolute;
    top:1%;
    right:0%;
    transform: rotate(45deg);
}
.mui-input-group1 {
    width:90%;
    height: 295px;
    border:1px solid white;
    margin:39px auto;
    padding-top:30px;
    padding-left: 15px;
}
.mui-input-row{
    height: 40px;
    border:1px solid white;
    position:relative;
    margin-top: 10px;
}
::-webkit-input-placeholder {/*Chrome/Safari*/
    color:#c0c6d4;
}
