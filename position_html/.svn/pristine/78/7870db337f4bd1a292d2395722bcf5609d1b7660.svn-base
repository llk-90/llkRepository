.top{
    width: 90%;
    height:3.5rem;
    margin-left: 5%;
    margin-top: 3%;
    display: flex;
    justify-content: space-around;
    background-color:white ;
}

.icon{
    width: 10%;
    height: 90%;
    margin-top: 1%;
    margin-left: 5%;
    display: flex;
    flex-direction: column;
    justify-content: space-around;
}

.text{
    width: 55%;
    height: 90%;
    margin-top: 1%;
    margin-left: 5%;
    padding-top: 0.8rem;
    display: flex;
    justify-content: flex-start;
}

.replace{
    width: 15%;
    height: 50%;
    margin-top: 3%;
    margin-left: 5%;
    padding-top: 1%;
}

.content{
    width: 95%;
    height:100%;
    margin-left: 2.5%;
    margin-top: 1%;
    display: flex;
    justify-content: space-around;
    flex-direction: column;
}

.from-child{
    display: flex;
    justify-content: flex-start;
    width: 100%;
    padding-left: 3%;
}

.from-child-label{
    border-right: 1px solid gainsboro;
    width: 100%;
    height: 80%;
    line-height: inherit;
    font-size: larger;
    display: flex;
    flex-direction: column;
    justify-content: space-around;
}
.from-child-child{
    width: 60%;
    height: 4rem;
    display: flex;
    flex-direction: column;
    justify-content: space-around;
    text-align: center;
}





