.time{
    width: 90%;
    height: 100%;
    margin-left: 5%;
    margin-top: 3%;
    display: flex;
    flex-direction: column;
    justify-content: space-around;
    z-index:999
}

.button-group {
    position: absolute;
    bottom: 20px;
    right: 20px;
    font-size: 12px;
    padding: 10px;
}
.amap-zoomcontrol{
    position: absolute;
    height: 50px;
    top:-380px;
    right:100px;

}

.amap-touch-toolbar .amap-geo{
    position: absolute;
    height: 50px;
    top:-420px;
}

.amap-pancontrol{
    position: absolute;
    height: 50px;
    top:-480px;
    right:-6px;
}
.amap-zoom-ruler{
    left: 11px;
}
.mui-backdrop {
    position: fixed;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    z-index: 998;
    background-color: rgba(0,0,0,.3);
}

