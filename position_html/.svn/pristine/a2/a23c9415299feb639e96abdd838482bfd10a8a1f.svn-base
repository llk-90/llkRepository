}
.fath{
	width:94%;
	height:33px;
	border:0px solid red;
	position: absolute;
	bottom:65px;
	left:3%;
	z-index: 999;
}
.son {
	height:32px;
	width:20%;
	border: 0px solid red;
	background-color: white;
	float:left;
	position: relative;
	box-shadow:0px -2px 1px #f6f6f6;
}
.son input{
	height:14px;
	width:14px;
	margin-top: 9px;
	margin-left:8px;
	border:none;
	outline:none;
}
.son label{
	height:16px;
	width:30px;
	font-size:12px;
	color:#807e7e;
	position: absolute;
	top:5px;
	right:5px;
	border:0px solid red;
}
.son img{
	width:24px;
	height:24px;
	margin-top: 4px;
	margin-left:4px;
}




