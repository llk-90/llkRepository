.tabBar{
    border-top: 1px solid #dcdce3;
    width:100%;
    height:58px;
    position: fixed;
    bottom: 0;
    box-shadow:0px -2px 3px #dcdce3;
    background-color:white;
    z-index:999;

}
.Modular1{
    border:0px solid red;
    margin-top:0;
    width:40%;
    height: 100%;
    padding-left: 0;
    position: absolute;
    left:0px;
}

.Modular1 img{
    border:0px solid red;
    width: 46%;
    height: 50%;
    margin-left:27%;
}
.Modular1 li{
    width: 40%;
    height: 84%;
    border:0px solid blue;
    float: left;
    list-style-type:none;
    margin-top:4%;
    margin-left: 2%;

}
.Modular1 span{
    text-align: center;
    display: block;
    border:0px solid green;
    font-size:12px;
    width:100%;
    line-height:16px;
    height:16px;
    color: #cdcdcd;
}
#aaa{
    margin-left:16%;
}

.img{
    width:310px;
    height:310px;
    border:60px solid red;
    z-index:9999;
    position:absolute;
    left:6%;
    top:78%;
    border-radius:50%;
    overflow: hidden;
}
#img{
    transform: rotate(60deg);
}

