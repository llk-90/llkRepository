/*header,*/


/*header>ul,*/

header>ul>li {
    display: block;
    height: 100%;
}

header {
    /*opacity: 0.99;*/
}

header,
header>ul {
    padding: 0!important;
    margin: 0!important;
}

header>ul>li:first-child {
    border-right: 0.5px solid gainsboro;
}

header>ul>li:last-child {
    border-left: 0.5px solid gainsboro;
}

header>ul>li {
    float: left;
    width: 50%;
    text-align: center;
    line-height: 44px;
    background-color: #F5F5F5;
    color: #C0C0C0;
    height: 44px;
    overflow: hidden;
}

.lihover{
    background-color: gainsboro;
    height: auto;
    box-shadow: 0 2px 10px rgba(0,0,0,.2);
    overflow: inherit;
}


//////////
header>ul>li>ul,
header>ul>li>ul>li {
    display: block;
    height: 100%;
}

header>ul>li>ul {
    max-height: 350px;
    overflow-y: auto;
    padding: 0;
}

header>ul>li>ul>li {
    background-color: white;
    list-style: none;
    opacity: 0.99;
    width: 100%;
}

header>ul>li>ul>li:active {
    background-color: gainsboro;
}

