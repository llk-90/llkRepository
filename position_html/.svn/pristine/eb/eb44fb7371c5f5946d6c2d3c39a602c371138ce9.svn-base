    <script type="text/javascript">
        function getParams() {
            var result = {};
            var str = decodeURI(window.location.search);
            //          alert(str);
            //          var strs = str.substring(1, str.length).split("&");
            var strs = str.split("?");
            if(strs && strs.length > 1) {
                strs = strs[1].split("&");
                for(i in strs) {
                    var v = strs[i];
                    var vs = v.split("=");
                    result[vs[0]] = vs[1] || '';
                    //                  console.log(i+":"+strs[i]);
                }
            }
            return result;
        }
        //      alert(getParams().openid);
    </script>

