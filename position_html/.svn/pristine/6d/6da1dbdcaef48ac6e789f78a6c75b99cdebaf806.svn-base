<script type="text/javascript">
    //点击事件相应
    function romoteClick(type) {
        switch (type)
        {
            case 'xiumian':
            {
                alert("即将开启远程休眠");
                
                postNetworkRequest("1");

            }
                break;
            case 'jiantinga':
            {
                alert("即将开启家长1监听");

                postNetworkRequest("2");               
            }
                break;
            case 'jiantingb':
            {
                alert("即将开启家长2监听");
               
                postNetworkRequest("3");
            }
                break;
            case 'close':
            {
                alert("即将开启远程关机");
                
                postNetworkRequest("4");
            }
                break;
            case 'recover':
            {
                alert("即将开启恢复出厂设置");
                
                postNetworkRequest("5");
            }
                break;
            case 'huanxin':
            {
                alert("即将开启远程唤醒");
                
                postNetworkRequest("6");
            }
                break;
            case 'back':
            {
                history.back();
            }
                break;

        }
    }
    function GetQueryString(name) {
        var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)");
        var r = window.location.search.substr(1).match(reg);
        if(r != null) return unescape(r[2]);
        return null;
    }
    //发起网络请求
    function postNetworkRequest(temp) {
        var openid = GetQueryString('openid');
        var stuid = GetQueryString('stuid');
        mui.init();

	    setTimeout(function() {
	        mui.ajax("/zhangzongbao/remoteControlApp/RemoteControl.webapp", {
	            data:{
	                openid : openid,
	                stuId :stuid,
	                temp:temp     
	            },
	            dataType: 'json', //服务器返回json格式数据
	            type: 'post', //HTTP请求类型
	            timeout: 10000, //超时时间设置为10秒；
	            success: function(data) {
	                if(data.statCode==="200"){
	                    showToast("请求发送成功");
	                }
	                else {
	                    showToast("请求发送失败");
	                }
	            },
	            error: function(xhr, type, errorThrown) {
	                showToast("请求发送失败");
	            }
	
	        });
	    }, 100);
	}
</script>

