header,
header>ul,
header>ul>li {
    display: block;
    height: 100%;
}

header {
    opacity: 0.99;
}

header,
header>ul {
    padding: 0!important;
    margin: 0!important;
}

header>ul>li:first-child {
    border-right: 0.5px solid gainsboro;
}

header>ul>li:last-child {
    border-left: 0.5px solid gainsboro;
}

header>ul>li {
    float: left;
    width: 50%;
    text-align: center;
    line-height: 44px;
    background-color: #F5F5F5;
    color: #C0C0C0;
    height: 44px;
    overflow: hidden;
}

/*header>ul>li:hover {
    background-color: gainsboro;
    height: auto;
    box-shadow: 0px 2px orange;
    overflow: inherit;
}*/


.lihover{
    background-color: gainsboro;
    height: auto;
    /*box-shadow: 0px 2px orange;*/
    box-shadow: 0 2px 10px rgba(0,0,0,.2);
    overflow: inherit;
}

//////////
header>ul>li>ul,
header>ul>li>ul>li {
    display: block;
    height: 100%;
}

header>ul>li>ul {
    max-height: 350px;
    overflow-y: auto;
    padding: 0;
}

header>ul>li>ul>li {
    background-color: white;
    list-style: none;
    opacity: 0.99;
    width: 100%;
}

header>ul>li>ul>li:active {
    background-color: gainsboro;
}

////
.self-mui-table-view-cell {
    padding-top: 0!important;
    padding-bottom: 0!important;
    padding-left: 0 !important;
}

.self-mui-table-view-cell>div {
    height: 70px;
    overflow: hidden;
}

.self-mui-table-view-cell p {
    max-width: 200px;
    margin-left: 15px;
}

.self-mui-table-view-cell>div>img {
    width: 100%;
}

.txt-download {
    line-height: 70px!important;
    text-align: center;
}

.mui-bar-nav {
    box-shadow: 0 0 white !important;
}

.mask {
    width: 100%;
    height: 100%;
    background: rgba(0, 0, 0, 0.6);
    visibility: hidden;
    position: absolute;
    z-index: 1;
    top: 0;
    left: 0;
}

.visibilityShow {
    visibility: visible!important;
}